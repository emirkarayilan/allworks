#include <stdio.h>

int main() {
    int num, a = 0, b = 1, c = 0;

    printf("Enter a number: ");
    scanf("%d", &num);

    int i = 1;

    while (i <= num) {
        c += a;
        int d = a + b;
        a = b;
        b = d;
        i++;
    }

    printf("Total sum of Fibonacci series up to term %d: %d\n", num, c);

    return 0;
}
